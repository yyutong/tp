@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title After command "add a/100 c/Entertainment t/1 d/Movie"

package States <<rectangle>> {
    class State1 as "__ab0:ExpenseBook__"
    class State2 as "__ab1:ExpenseBook__"
    class State3 as "__ab2:ExpenseBook__"
}

State1 -[hidden]right-> State2
State2 -[hidden]right-> State3

class Pointer as "Current State" #FFFFF

Pointer -up-> State3
@end
